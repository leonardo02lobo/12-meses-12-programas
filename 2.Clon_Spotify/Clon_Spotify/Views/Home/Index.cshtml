	};
	string nameOfTheAlbum = "Name of The Album";
	string artist = "Artist";
}
<section class="index">
	<header>
		<button class="btn-listado">
			All
		</button>
		<button class="btn-listado">
			Music
		</button>
	</header>
	<main class="main">
		<section class="Escuchados-Recientes container">
			@for(int i = 0; i < 8; i++){
				<component type="typeof(TarjetaMasEscuchada)" render-mode="ServerPrerendered" />
			}
		</section>
		<section class="playlist-reciente">
			<div>
				<a href="#"><h3>Recently played</h3></a>
				<a href="#">Show all</a>
			</div>
			<section class="carrucel-tarjeta">
				<div class="div-grande" id="divGrande">
					@foreach (string url in urls)
					{
						<component type="typeof(TarjetaPlayList)" render-mode="ServerPrerendered" param-urlImage=@url param-description="@nameOfTheAlbum" />
					}
				</div>
				<ul>
					<li id="li1"><component type="typeof(FlechaIzquierdaPlayList)" render-mode="ServerPrerendered" /></li>
					<li id="li1"><component type="typeof(FlechaDerechaPlayList)" render-mode="ServerPrerendered"/></li>
				</ul>
			</section>
		</section>
		<section class="playlist-reciente">
			<div>
				<a href="#"><h3>Recently played</h3></a>
				<a href="#">Show all</a>
			</div>
			<section class="carrucel-tarjeta">
				<div class="div-grande" id="divGrande">
					@foreach (string url in artists)
					{
						<component type="typeof(TarjetaPlayList)" render-mode="ServerPrerendered" param-urlImage=@url param-description="@artist" />
					}

				</div>
				<ul>
					<li id="li2"><component type="typeof(FlechaIzquierdaPlayList)" render-mode="ServerPrerendered" /></li>
					<li id="li2"><component type="typeof(FlechaDerechaPlayList)" render-mode="ServerPrerendered" /></li>
				</ul>
			</section>
		</section>
	</main>
</section>